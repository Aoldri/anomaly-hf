igi_actions.conditions.can_get_object = function (cond)
    -- print("can_get_object(cond)")
	local item = igi_helper.level_object(cond.id)
	if item then
        -- print("can_get_object(cond) - TRUE")
        return true
    else
        -- print("can_get_object(cond) - FALSE")
        return false
    end
end