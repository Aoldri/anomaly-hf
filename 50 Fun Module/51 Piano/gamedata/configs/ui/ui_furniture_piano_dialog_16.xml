<w>
    <!-- Sheet -->
    <sheet_bg x="270" y="60" width="484" height="310" stretch="1">
        <texture>ui_inGame2_hint_wnd_main_window</texture>
    </sheet_bg>

     <title x="0" y="0" width="464" height="50" stretch="1" complex_desc="0">
        <text font="letterica25" complex_mode="1" align="l" vert_align="l"/>
    </title>

    <score_scroll x="10" y="10" width="464" height="280" stretch="1" complex_desc="0">
    </score_scroll>

    <score x="0" y="0" width="434" height="280" stretch="1" complex_desc="0">
        <text font="letterica16" complex_mode="1" align="l" vert_align="l"/>
    </score>

    
    <!-- Controls -->
    <autoplay_btn x="256" y="373" width="93" height="22" stretch="1">
            <texture>ui_checkbox_TabLED</texture>
            <txt x="15" y="0" width="93" height="22" stretch="1">
                <text complex_mode="1" r="200" g="200" b="200" align="l"  vert_align="c" font="letterica16"/>
            </txt>
    </autoplay_btn>

    <piano_config_btn x="677" y="373" width="93" height="22" stretch="1">
            <texture>ui_checkbox_TabLED</texture>
            <txt x="15" y="0" width="93" height="22" stretch="1">
                <text complex_mode="1" r="200" g="200" b="200" align="l"  vert_align="c" font="letterica16"/>
            </txt>
    </piano_config_btn>

    <piano_config_bg x="773" y="372" width="246" height="201" stretch="1">
        <texture>ui_icons_PDA_dialog</texture>
    </piano_config_bg>
    <piano_config_scroll x="4" y="4" width="238" height="197" stretch="1" complex_desc="0">
    </piano_config_scroll>

    <trackbar_container x="12" y="12" width="238" height="24">
        <caption x="0" y="0" width="60" height="24" stretch="1">
            <text r="170" g="170" b="170" font="letterica16" align="l" vert_align="c"/>
        </caption>
        <trackbar x="64" y="4" width="140" height="16" stretch="1"/>
        <value x="208" y="0" width="30" height="24" stretch="1">
            <text r="170" g="170" b="170" font="letterica16" align="l" vert_align="c"/>
        </value>
    </trackbar_container>

    <select_container x="12" y="12" width="238" height="24">
        <caption x="0" y="0" width="60" height="24" stretch="1">
            <text r="170" g="170" b="170" font="letterica16" align="l" vert_align="c"/>
        </caption>
        <select x="64" y="0" width="160" height="24" can_select="1">
            <select_font r="170" g="170" b="170" font="letterica16"/>
                <text_color>
                    <e r="170" g="170" b="170"/>
                    <d r="70" g="70" b="70"/>
                </text_color>
        </select>
    </select_container>

    <sheet_select x="270" y="34" width="256" height="24" can_select="1">
		<select_font r="170" g="170" b="170" font="letterica16"/>
            <text_color>
	            <e r="170" g="170" b="170"/>
	            <d r="70" g="70" b="70"/>
            </text_color>
    </sheet_select>

    <tag_select_btn x="661" y="36" width="93" height="22" stretch="1">
            <texture>ui_checkbox_TabLED</texture>
            <txt x="15" y="0" width="93" height="22" stretch="1">
                <text complex_mode="1" r="200" g="200" b="200" align="l"  vert_align="c" font="letterica16"/>
            </txt>
    </tag_select_btn>

    <tag_select_bg x="756" y="36" width="128" height="201" stretch="1">
        <texture>ui_icons_PDA_dialog</texture>
    </tag_select_bg>

    <tag_select_scroll x="4" y="4" width="120" height="193" stretch="1" complex_desc="0">
    </tag_select_scroll>

    <tag_select x="4" y="4" width="116" height="20">
        <cap_tag x="0" y="0" width="40" height="20" stretch="1"> <!-- midpoints: 8, 18 -->
            <text r="170" g="170" b="170" font="letterica16" align="l" vert_align="c"/>
        </cap_tag>		
        <check_tag x="85" y="0" width="31" height="20" stretch="1">
            <texture>ui_inGame2_checkbox</texture>
        </check_tag>
    </tag_select>
    


    <!-- Piano -->
    <piano_bg x="256" y="398" width="513" height="144" stretch="1">
        <texture>ui_inGame2_piano_bg</texture>
    </piano_bg>

    <white_key x="269" y="442" width="13.5" height="86" stretch="1">
        <texture>ui_inGame2_piano_whitekey_off</texture>
    </white_key>

    <black_key x="278" y="442" width="9" height="58" stretch="1">
        <texture>ui_inGame2_piano_blackkey_off</texture>
    </black_key>

    <!-- Pick up, Close btns -->
    <btn_pickup x="266" y="545" width="101" height="28" check_mode="0" stretch="1">
        <window_name>button_pickup</window_name>
        <text font="letterica18">st_pickup</text>
        <texture>ui_button_ordinary</texture>
    </btn_pickup>

    <btn_close x="657" y="545" width="101" height="28" check_mode="0" stretch="1">
        <window_name>button_close</window_name>
        <text font="letterica18">st_close</text>
        <texture>ui_button_ordinary</texture>
    </btn_close>
</w>